<h1 class="center">Listing all Articles: </h1>




<% if @articles.length > 0 %>
    <div class="container">
        <div class="center"><%= will_paginate %></div>
        <%=  render "article", obj: @articles %>
    </div>
<%else%>
    <p>There is no Article..! would you like to <%= link_to 'create new article', new_article_path %></p>
<%end%>